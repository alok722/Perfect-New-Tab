{
	"manifest_version": 2,
	"name": "Perfect Tab",
	"manifest_version": 2,
	"version": "1.0",
	"chrome_url_overrides" : {
	    "newtab": "perfect-tab.html"
	  },
	"background": {
	    "scripts": ["perfect-tab.js"],
	    "persistent": false
	  },
	// "icons": { "16": "icons/icon16.png",
 //           "32": "icons/icon32.png",
 //          "128": "icons/icon128.png" },
  	"permissions" : [
  		"tabs",
  		"bookmarks",
  		"storage",
  		"downloads",
  		"history",
  		"sessions",
  		"topSites",
  		"chrome://favicon/"
  	]
}