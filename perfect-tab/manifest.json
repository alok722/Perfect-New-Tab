{
	"manifest_version": 3,
	"name": "Perfect Tab",
  "description": "This extension replaces the new tab page with a more feature-rich and lightweight page.",
	"version": "3.0",
	"chrome_url_overrides" : {
	    "newtab": "perfect-tab.html"
	  },
    "icons": {"48": "icon.png"},
  	"permissions" : [
  		"tabs",
  		"bookmarks",
  		"storage",
  		"downloads",
  		"history",
  		"sessions",
  		"topSites",
  		"chrome://favicon/"
  	]
}